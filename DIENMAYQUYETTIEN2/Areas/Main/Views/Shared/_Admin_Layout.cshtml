    <link href="~/Content/sb-admin.css" rel="stylesheet" />
    <link href="~/Content/bootstrap1.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,500&amp;subset=vietnamese" rel="stylesheet">
    <link rel="stylesheet" href="https://sto.onlinefriday.vn/theme/frontend/default/lib/fontAwesome/css/fontawesome-all.css" media="all" />
    <link rel="stylesheet" href="https://sto.onlinefriday.vn/theme/frontend/default/lib/bootstrap/css/bootstrap.min.css" media="all" />
